<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	x:Class="GymTracker.View.ImportExportPage"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:model="clr-namespace:GymTracker.Model"
	xmlns:viewmodel="clr-namespace:GymTracker.ViewModel"
	Title="ImportExportPage"
	x:DataType="viewmodel:ImportExportPageViewModel"
	Style="{DynamicResource PageStyle}">

	<StackLayout>
		<Button Command="{Binding LoadFileCommand}" Text="Import" />
		<Button Command="{Binding SaveFileCommand}" Text="Export" />
	</StackLayout>
</ContentPage>